{
    "name": "inthernals-kit",
    "version": "1.0.0",
    "description": "The starter kit contains components, fonts, pages, scripts, and styles using tailwindcss from inthernals",
    "main": "tailwind.config.js",
    "scripts": {
        "tw-build": "npx tailwindcss -i ./source@inthernals-kit.css -o ./styles/inthernals-kit.css",
        "tw-watch": "npx tailwindcss -i ./source@inthernals-kit.css -o ./styles/inthernals-kit.css --watch",
        "tw-minify": "npx tailwindcss -o ./styles/inthernals-kit.css --minify"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/inthernals/inthernals-kit.git"
    },
    "keywords": [
        "Inthernals kit"
    ],
    "author": "Inthernals Team",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/inthernals/inthernals-kit/issues"
    },
    "homepage": "https://github.com/inthernals/inthernals-kit#readme",
    "devDependencies": {
        "tailwindcss": "^3.2.7"
    }
}
